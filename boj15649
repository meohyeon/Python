n, m = map(int,input().split())
lis = [0]*(n+1)
arr = [0]*(n+1)
def back(a):
    if a == m:
        for i in range(m):
            print(arr[i],end=" ")   
        print(end="\n")
        return
    for i in range(1,n+1):
        if lis[i] != 1:
            arr[a] = i
            lis[i]=1
            back(a+1)
            lis[i]=0
    



back(0)